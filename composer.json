{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.2",
        "barryvdh/laravel-dompdf": "^2.1",
        "beyondcode/laravel-websockets": "^2.0@beta",
        "bmatovu/laravel-xml": "^3.0",
        "claviska/simpleimage": "^3.7",
        "doctrine/dbal": "^3.8",
        "fabpot/goutte": "^4.0",
        "fruitcake/laravel-cors": "^2.0",
        "google/apiclient": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "itsjavi/bootstrap-colorpicker": "^3.4",
        "lab404/laravel-impersonate": "^1.7",
        "laravel/framework": "^8.75",
        "laravel/sanctum": "^2.15",
        "laravel/socialite": "^5.12",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.4",
        "maatwebsite/excel": "^3.1",
        "marxolity/open-ai": "^1.2",
        "mckenziearts/laravel-notify": "^2.3",
        "pusher/pusher-php-server": "^7.2",
        "spatie/array-to-xml": "^2.17",
        "spatie/browsershot": "^3.58",
        "spatie/laravel-activitylog": "^3.17",
        "spatie/laravel-backup": "^6.16",
        "spatie/laravel-feed": "^3.2",
        "spatie/laravel-permission": "^5.11",
        "spatie/laravel-searchable": "^1.11",
        "spatie/laravel-settings": "^3.3",
        "spatie/ssh": "^1.9",
        "spatie/valuestore": "^1.3",
        "symfony/browser-kit": "^5.4",
        "symfony/process": "^5.4"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/dusk": "^6.25",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
